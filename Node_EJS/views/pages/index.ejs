<!DOCTYPE html>
<html lang="pt-br">

<%- include('../partials/head'); %> <!--sintaxe do ejs-->

<body class="container">

    <%- include('../partials/header'); %>

    <main>
        <div class="jumbotron">
            <h1>Node com EJS</h1>
            <p>Vamos aprender a trabalhar com arquivos EJS</p>
            <p><%= subtitle %></p>
        </div>

        <ul>
            <% qualitys.forEach(function(quality) { %>
                <li>
                    <strong><%= quality.title %></strong> <%= quality.message %>
                </li>
            <% }) %>
        </ul>
    </main>

    <%- include('../partials/footer'); %>

</body>

</html>
